		<secure-properties:encrypt algorithm="Blowfish" />
	</secure-properties:config>
	<http:request-config name="Salesforce_HTTP_Request_configuration" doc:name="HTTP Request configuration" doc:id="c1dbbec4-9d5e-47e6-8800-0b9a098c2482" >
		<http:request-connection host="${secure::salesforce.host}" port="${secure::salesforce.port}" />
	</http:request-config>
